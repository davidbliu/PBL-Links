<h1>Manage Members</h1>
<br>
<table class='table table-striped'>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Actions</th>
    </tr>
  </thead>

  <tbody>
    <% @members.each do |member| %>
      <tr>
        <td><%= member.name %></td>
        <td><%= member.email %></td>
        <td><a href = '/members/<%= member.id %>/destroy'>Destroy</a> | <a href = '/members/<%= member.id %>/edit'>Edit</a></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

