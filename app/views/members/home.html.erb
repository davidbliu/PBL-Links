
<style type = 'text/css'>
.dashboard-panel-title{
	color:white;
}
.dashboard-panel-blue{
	background-color: black;
}
</style>
<div class = 'row'>

	<div class = 'col-md-4'>

		<div class="panel panel-default panel-material-blue-700">
		  <div class="panel-heading">
		    <h3 class="panel-title dashboard-panel-title"><span class = 'mdi-notification-folder-special'></span>&nbsp;Starred Resources</h3>
		  </div>
		  <div class="panel-body">
		    <ul>
		    	<% @trending_links.each do |link| %>
		    		<li><a href = '<%= link.url %>'>pbl.link/<%= link.key %></a></li>
		    		<% end %>
		    </ul>
		  </div>
		</div>	



		<div class="panel panel-default panel-material-blue-700">
		  <div class="panel-heading">
		    <h3 class="panel-title dashboard-panel-title"><span class = 'mdi-social-notifications'></span>&nbsp;Announcements</h3>
		  </div>
		  <div class="panel-body">
		    <ul>
		    	<li>PD Challenge ends tomorrow!</li>
		    </ul>
		  </div>
		</div>	

		<div class="panel panel-default panel-material-blue-700" >
		  <div class="panel-heading">
		    <h3 class="panel-title dashboard-panel-title"><span class = 'mdi-action-toc'></span>&nbsp;TO DO</h3>
		  </div>
		  <div class="panel-body">
		    <ul>
		    	<li>Sign up for 3 Concessions</li>
		    	<li>Fill out mid-semester survey</li>
		    	<li>Pay for Yearbook</li>
		    </ul>
		  </div>
		</div>	
</div>
</div>
