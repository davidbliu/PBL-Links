<h1>Welcome to PBL Members Portal</h1>

<table class='table table-condensed table-hover table-bordered'>
  <thead>
    <tr>
      <th>name</th>
      <th>committee</th>
      <th>email</th>
      <th>phone</th>
      <th>position</th>
      <th>semesters</th>
    </tr>
  </thead>

  <tbody>
    <% @current.each do |data| %>
      <tr>
        <td><%= data['name'] %></td>
        <td><%= data['committee'] %></td>
        <td><%= data['email'] %></td>
        <td><%= data['phone'] %></td>
        <td><%= data['position'] %></td>
        <td><%= data['semester'] %></td>
      </tr>
    <% end %>

    <% @alumni.each do |data| %>
      <tr>
        <td><%= data['name'] %></td>
        <td>alumni</td>
        <td><%= data['email'] %></td>
        <td><%= data['phone'] %></td>
        <td><%= data['position'] %></td>
        <td><%= data['semester'] %></td>
      </tr>
    <% end %>


  </tbody>
</table>

<br>

