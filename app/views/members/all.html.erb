<%= javascript_include_tag "jquery.tablesorter", "data-turbolinks-track" => true %>
<%= javascript_include_tag "jquery-latest", "data-turbolinks-track" => true %>

<h1>Welcome to PBL Members Portal</h1>
<br>
<h2>Current Members</h2>
<table class='table table-striped' >
  <thead>
    <tr>
      <th>name</th>
      <th>committee</th>
      <th>email</th>
      <th>phone</th>
      <th>position</th>
      <th>semesters</th>
    </tr>
  </thead>

  <tbody>
    <% @current.each do |data| %>
      <tr>
        <td><%= data['name'] %></td>
        <td><%= data['committee'] %></td>
        <td><%= data['email'] %></td>
        <td><%= data['phone'] %></td>
        <td><%= data['position'] %></td>
        <td><%= data['semester'] %></td>
      </tr>
    <% end %>

  </tbody>
</table>
<br>
<h2>Alumni</h2>
<table class='table table-striped' >
  <thead>
    <tr>
      <th>name</th>
      <th>email</th>
      <th>phone</th>


    </tr>
  </thead>

  <tbody>
    <% @alumni.each do |data| %>
      <tr>
        <td><%= data['name'] %></td>
        <td><%= data['email'] %></td>
        <td><%= data['phone'] %></td>

      </tr>
    <% end %>

  </tbody>
</table>
<br>

