

<div>
<table>
<tr>
<% @slots.keys.sort.each do |tabling_day| %>
	<td>
		<div class = 'tabling-day'>
		<% @slots[tabling_day].each do |tabling_slot| %>
			<div class = 'tabling-slot' id = '<%= tabling_slot.id %>'>
				<h4 class = 'tabling-slot-label'><%= tabling_slot.time_string %></h4>
				<% tabling_slot.member_emails.each do |email| %>
					<div style = 'margin-bottom:5px'><img src = '<%= member_email_hash[email].gravatar_url %>' class ='profile-image profile-image-small'></img>&nbsp;&nbsp;<%= member_email_hash[email].name %></div>
				<% end %>
			</div>
		<% end %>
		</div>
	</td>
<% end %>
</tr>
</table>
</div>