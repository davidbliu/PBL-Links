<h1 class = 'centered'>Attendance</h1>

<table class = 'table table-striped'>
	<thead>
		<tr>
			<th>Event</th>
			<% @members.each do |member| %>
			<th><%= member.name %></th>
			<% end %>
		</tr>
	</thead>
	<tbody>
		<% @events.each do |event| %>
		<tr>
			<td><%= event.name %></td>
			<% @members.each do |member| %>
			<td><% if @event_members.include?(event.google_id + ',' + member.email) %>x<% end %></td>
			<% end %>
		</tr>
		<% end %>
	</tbody>
</table>

