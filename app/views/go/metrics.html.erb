<h1><span class = 'mdi-action-trending-up' style = 'font-size:50px'></span>&nbsp;<%= @golink.key %></h1>
<div class = 'row'>
	<div class = 'col-md-6'>
	<table class = 'table table-condensed'>
		<thead>
			<th>Member</th>
			<th>Time</th>
		</thead>
		<tbody>
			<% @clicks.each do |click| %>
			<tr>
				<td><% if click.member_email %>
					<%= click.member_email %>
					<% else %>
					Not Signed In
					<% end %>
				</td>
				<td><%= click.time_string %></td>
			</tr>
			<% end %>
		</tbody>
	</table>
</div>
</div>