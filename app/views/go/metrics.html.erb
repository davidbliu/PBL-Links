<%= link_to 'Back to Catalogue', '/go' %>
<h1><%= @golink.key %> Metrics</h1>
<div class = 'row'>
	<div class = 'col-md-6'>
	<table class = 'table table-condensed'>
		<thead>
			<th>Member Name</th>
			<th>Time Clicked</th>
		</thead>
		<tbody>
			<% @clicks.each do |click| %>
			<tr>
				<td><% if click.member_id %>
					<%= @member_hash[click.member_id].name %>
					<% else %>
					None
					<% end %>
				</td>
				<td><%= click.updated_at %></td>
			</tr>
			<% end %>
		</tbody>
	</table>
</div>
</div>